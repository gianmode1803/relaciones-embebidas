<p id="notice"><%= notice %></p>

<h1>Coach Team Nns</h1>

<table>
  <thead>
    <tr>
      <th>Coach</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coach_team_nns.each do |coach_team_nn| %>
      <tr>
        <td><%= coach_team_nn.coach.nombre %></td>
        <td><%= coach_team_nn.team.nombre %></td>
        <td><%= link_to 'Show', coach_team_nn %></td>
        <td><%= link_to 'Edit', edit_coach_team_nn_path(coach_team_nn) %></td>
        <td><%= link_to 'Destroy', coach_team_nn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

